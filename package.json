    "scripts": {
        "start": "grunt set-wallet && grunt generate-config && grunt build",
        "dev": "yarn start && cross-env NODE_ENV=development DEVMOD=\"true\" LOG=\"false\" webpack-dev-server --hot",
        "dev-p": "yarn start && cross-env NODE_ENV=development DEVMOD=\"true\" LOG=\"false\" webpack-dev-server --hot --progress --debug ",
        "build": "grunt set-wallet && cross-env NODE_ENV=production DEVMOD=\"false\" LOG=\"true\" grunt \"build:webpack:prod\"",
        "build-debug": "grunt set-wallet && cross-env NODE_ENV=development DEVMOD=\"false\" LOG=\"true\" grunt \"build:webpack:prod\"",
        "deploy": "yarn build &&  surge www",
        "android-r": "grunt set-wallet && cross-env NODE_ENV=production DEVMOD=\"false\" LOG=\"true\" grunt \"build:build android r\"",
        "android-r-d": "grunt set-wallet && cross-env NODE_ENV=production DEVMOD=\"true\" LOG=\"true\" grunt \"build:build android r\"",
        "android": "grunt set-wallet && cross-env DEVMOD=\"true\" LOG=\"true\" grunt \"build:build android\"",
        "ios-r": "grunt set-wallet && cross-env DEVMOD=\"false\" LOG=\"true\" NODE_ENV=production grunt \"build:build ios r\"",
        "ios-r-d": "grunt set-wallet && cross-env NODE_ENV=production DEVMOD=\"true\" LOG=\"true\" grunt \"build:build ios r\"",
        "ios": "grunt set-wallet && cross-env DEVMOD=\"true\" LOG=\"true\" grunt \"build:build ios\"",
        "all-r": "grunt set-wallet && cross-env DEVMOD=\"false\" LOG=\"true\" NODE_ENV=production grunt \"build:build all r\"",
        "all-r-d": "grunt set-wallet && cross-env NODE_ENV=production DEVMOD=\"true\" LOG=\"true\" grunt \"build:build all r\"",
        "webpack-build": "yarn start && cross-env NODE_ENV=development DEVMOD=\"true\" LOG=\"true\" webpack",
        "lint": "eslint CORE/ projects/",
        "lint-fix": "eslint CORE/ projects/ --fix",
        "mwallet-init": "grunt mwallet-init",
        "all-store": "grunt set-wallet && cross-env DEVMOD=\"false\" LOG=\"true\" NODE_ENV=production grunt \"build:build all store\"",
        "web-upload": "yarn start && cross-env DEVMOD=\"false\" LOG=\"true\" NODE_ENV=production grunt \"build:build web\"",
        "all-demo": "grunt set-wallet && cross-env DEVMOD=\"true\" LOG=\"true\" NODE_ENV=production grunt \"build:build all store\""
