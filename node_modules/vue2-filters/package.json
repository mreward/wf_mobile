{
  "name": "vue2-filters",
  "version": "0.4.1",
  "description": "The list of standard filters Vue.js 1.* adapted for use in Vue.js 2.*",
  "main": "dist/vue2-filters.js",
  "scripts": {
    "build": "node_modules/.bin/webpack && node_modules/.bin/uglifyjs dist/vue2-filters.js -o dist/vue2-filters.min.js",
    "dev": "node_modules/.bin/webpack --watch",
    "test": "node_modules/.bin/karma start test/karma.conf.js --single-run --no-auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/freearhey/vue2-filters.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "filters",
    "vue2-filters"
  ],
  "author": "Arhey",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "babel-loader": "8.0.4",
    "babel-preset-env": "1.7.0",
    "jasmine": "3.3.0",
    "karma": "3.1.1",
    "karma-jasmine": "2.0.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.5",
    "uglify-es": "3.3.9",
    "webpack": "4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "peerDependecies": {
    "vue": "^2.0.0"
  }
}
